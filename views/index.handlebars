<div class="row">
  {{#each ars}}
    <div class="col s12 m4">
      <div class="card small  blue-grey lighten-5">
        <div class="card-content gray-text">
          <span class="card-title"><a href="{{link}}" target="_blank" class="headline">{{title}}</a></span>
          <p class='byline'>{{author}}</p>
          <p class='summary'>{{summary}}</p>
        </div>
        <div class="card-action">
          <a id="save" data-id="{{ id }}" class="btn-small tool-tipped waves-effect waves-light" data-position="bottom" data-delay="50" data-tooltip="Save Article" href=""><i class="material-icons left">add_circle</i></a>
        </div>
      </div>
    </div>
  {{/each}}


</div>
<!-- Modal Structure -->
<div id="modal1" class="modal modal-fixed-footer">
  <div class='model-content'>
      <div id="notes"></div>
  </div>

</div>




</body>


